{
    "name": "sudo-ecommerce/image-domain-replace",
    "description": "Laravel middleware package to replace image domains and handle bucket checks.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sudo Team",
            "email": "dev@sudo.vn"
        }
    ],
    "require": {
        "php": ">=7.1 || ^8.0",
        "guzzlehttp/guzzle": "^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Sudo\\ImageDomainReplace\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sudo\\ImageDomainReplace\\ImageDomainReplaceServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}